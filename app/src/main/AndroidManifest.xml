            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".com.update.com.Update_Profile_Activity" />

        <service android:name=".com.login.com.Get_Device_Token">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".Search_Activity" />
        <activity android:name=".Choose_A_Driver" />
        <activity android:name=".FreeRide" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".Notification" />
        <activity
